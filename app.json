{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-logo.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Profil bilgilerinizi güncellemek için fotoğraflarınıza erişmek istiyoruz.",
          "NSCameraUsageDescription": "Bu uygulama fotoğraf çekmek için kamerayı kullanır."
        }
      ],
      "expo-secure-store"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Konumunuza erişmek istiyoruz.",
        "NSLocationAlwaysUsageDescription": "Konumunuza erişmek istiyoruz.",
        "NSCameraUsageDescription": "Fotoğraf çekebilmeniz için kameranıza erişim izni gerekiyor.",
        "NSPhotoLibraryUsageDescription": "Fotoğraf seçebilmeniz için fotoğraf galerisine erişim izni gerekiyor."
      },
      "bundleIdentifier": "com.anonymous.frontend"
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontend"
    },
    "web": {
      "favicon": "./assets/app-logo.png"
    }
  }
}
