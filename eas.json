{
  "cli": {
    "version": ">= 13.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "SENTRY_AUTH_TOKEN": "$EXPO_PUBLIC_SENTRY_AUTH_TOKEN"
        }
      },
      "ios": {
        "env": {
          "SENTRY_AUTH_TOKEN": "$EXPO_PUBLIC_SENTRY_AUTH_TOKEN"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
